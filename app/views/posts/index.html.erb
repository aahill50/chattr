<section class="index-profile">
  <%= render 'users/index_profile' %>
  <%= render 'posts/post_form', parent_post_id: nil %>
</section>
<section class="posts-container">
  <header class="title"><h2>Posts</h2></header>
  <% @posts.each do |post| %>
    <%= render 'posts/post', post: post %>
  <% end %>
</section>
